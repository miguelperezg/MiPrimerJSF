<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC 
"-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" 
"http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">

<struts-config>
	<form-beans>
		<form-bean name="BienvenidaForm" type="com.open4job.struts.controller.BienvenidaControlador" />
	</form-beans>
	<action-mappings>
		<action path="/bienvenida" type="com.open4job.struts.controller.Controlador"
		name="BienvenidaForm" input="/index.jsp">
			<forward name="Bienvenido" path="/Bienvenido.jsp" />
		</action>
	</action-mappings>

</struts-config>